<p id="notice"><%= notice %></p>

<h1>Listing Jwans</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jwans.each do |jwan| %>
      <tr>
        <td><%= link_to 'Show', jwan %></td>
        <td><%= link_to 'Edit', edit_jwan_path(jwan) %></td>
        <td><%= link_to 'Destroy', jwan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jwan', new_jwan_path %>
